{
  "name": "spotify-to-jekyll-action",
  "version": "1.0.0",
  "description": "Turn a Spotify playlist into a Jekyll post",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tape test/*.js",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "pretty-quick": "pretty-quick",
    "build": "ncc build src/action.js"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "devDependencies": {
    "@vercel/ncc": "^0.25.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "tape": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "spotify-to-jekyll": "^0.4.0",
    "spotify-web-api-node": "^4.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/spotify-to-jekyll-action.git"
  },
  "bugs": {
    "url": "https://github.com/katydecorah/spotify-to-jekyll-action/issues"
  },
  "homepage": "https://github.com/katydecorah/spotify-to-jekyll-action#readme"
